{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/images/*", "/*.pdf", "/*.png", "/*.ico", "/*.svg"]
  },
  "routes": [
    {
      "route": "/Resume_Rutao_Luo.pdf",
      "headers": {
        "Content-Type": "application/pdf",
        "Content-Disposition": "attachment; filename=\"Resume_Rutao_Luo.pdf\""
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404/index.html",
      "statusCode": 404
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "X-XSS-Protection": "1; mode=block",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "camera=(), microphone=(), geolocation=()"
  },
  "mimeTypes": {
    ".pdf": "application/pdf"
  }
}
